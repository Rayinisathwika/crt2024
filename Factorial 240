#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int num,sum=0,factorial=1;
    scanf("%d",&num);
    while(num>=10){
     while(num!=0){
       sum+=num%10;
        num/=10;
     }
      num=sum;
      sum=0;
    }
  for(int i=2;i<=num;i++){
  factorial*=i;
  }
   printf("%d",factorial);
   return 0;
}
